{
  "name": "rasengan",
  "private": false,
  "version": "1.0.0-beta.37",
  "description": "The modern frontend framework for React",
  "type": "module",
  "main": "lib/esm/index.js",
  "bin": {
    "rasengan": "./lib/esm/cli/index.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/types/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "types": "./lib/types/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "types": "./lib/types/index.d.ts",
      "default": "./lib/esm/index.js"
    },
    "./types/client": {
      "types": "./types/client.d.ts"
    },
    "./package.json": "./package.json"
  },
  "author": {
    "name": "dilane3",
    "email": "komboudilane125@gmail.com",
    "url": "https://dilane3.com",
    "twitter": "https://twitter.com/dilanekombou",
    "github": "https://github.com/dilane3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rasengan-dev/rasenganjs.git",
    "issues": "https://github.com/rasengan-dev/rasenganjs/issues"
  },
  "homepage": "https://rasengan.dev",
  "scripts": {
    "dev": "node server",
    "preview": "cross-env NODE_ENV=production node server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr node_modules/rasengan/lib/esm/entries/entry-server.js --outDir dist/server",
    "compile": "tsc -b ./tsconfig.esm.json ./tsconfig.types.json",
    "build:lib:clean": "rm -rf ./lib",
    "build:lib:all": "npm-run-all build:lib:clean compile && node ./src/scripts/copy-extra-files.js",
    "deploy": "npm publish --access public",
    "deploy:beta": "npm run deploy --tag beta",
    "pack": "npm pack --pack-destination ./../packages/rasengan/"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "compression": "^1.7.4",
    "cross-spawn": "^7.0.3",
    "execa": "^8.0.1",
    "express": "^4.18.2",
    "file-type": "^19.0.0",
    "inquirer": "^9.2.12",
    "keypress": "^0.2.1",
    "node-fetch": "^3.3.2",
    "open": "^10.1.0",
    "ora": "^7.0.1",
    "path-browserify": "^1.0.1",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.20.1",
    "sirv": "^2.0.3"
  },
  "peerDependencies": {
    "@netlify/functions": "^2.6.0",
    "@types/node": "^18.0.0 || >=20.0.0",
    "less": "*",
    "react": "^18.3.*",
    "react-dom": "^18.3.*",
    "sass": "*",
    "stylus": "*",
    "vite": "^5.2.0"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    },
    "@netlify/functions": {
      "optional": true
    },
    "sass": {
      "optional": true
    },
    "stylus": {
      "optional": true
    },
    "less": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/compression": "^1.7.4",
    "@types/cross-spawn": "^6.0.4",
    "@types/express": "^4.17.19",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@vercel/node": "^3.0.14",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "@vitejs/plugin-react": "^4.2.1"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.19.0",
    "npm": ">=7.0.0"
  },
  "keywords": [
    "rasengan",
    "frontend",
    "framework",
    "react",
    "vite",
    "ssr"
  ]
}
